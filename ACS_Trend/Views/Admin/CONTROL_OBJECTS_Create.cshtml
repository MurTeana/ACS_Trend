@model ACS_Trend.Models.Control_objectViewModel

@{
    ViewData["Title"] = "CONTROL_OBJECTS_Create";
}

<h1>CONTROL_OBJECTS_Create</h1>
<h2>@ViewBag.Issuccess</h2>

<h4>Control_objectViewModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CONTROL_OBJECTS_Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Control_object_name" class="control-label"></label>
                <input asp-for="Control_object_name" class="form-control" />
                <span asp-validation-for="Control_object_name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Extend_information" class="control-label"></label>
                <input asp-for="Extend_information" class="form-control" />
                <span asp-validation-for="Extend_information" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Control_object_type.Control_object_type_name" class="control-label"></label>
                @Html.DropDownList("CO_ID_Control_object_type", ViewBag.Control_object_types as SelectList, htmlAttributes: new { @class = "form-control" })
                <span asp-validation-for="CO_ID_Control_object_type" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

