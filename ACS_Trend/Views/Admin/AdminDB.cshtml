@{
    ViewData["Title"] = "AdminDB";
}
<h1>Администрирование базы данных</h1>


<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Таблица базы данных</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.EntitiesList)
        {
            <tr>
                <td>@item.Id</td>
                <td><a asp-action="@item.Method">@item.Entity</a></td>
            </tr>
        }
    </tbody>
</table>
