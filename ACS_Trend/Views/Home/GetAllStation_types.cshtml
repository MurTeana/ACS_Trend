@model IEnumerable<ACS_Trend.Models.Station_typeViewModel>

@{
    ViewData["Title"] = "GetAllStation_types";
}

<h1>GetAllStation_types</h1>

<p>
    <a asp-action="CreateStation_type">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StationType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StationType)
            </td>
            <td>
                <a asp-action="EditStation_type" asp-route-id="@item.ID_Station_type">Edit</a> |
                <a asp-action="Details_Station_Type" asp-route-id="@item.ID_Station_type">Details</a> |
                <a asp-action="DeleteStation_type" asp-route-id="@item.ID_Station_type">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
